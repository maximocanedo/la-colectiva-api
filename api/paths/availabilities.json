            "application/json": {
              "schema": {
                "$ref": "./../schemas/schemas.json#/GeneralError"
              }
            }
          }
        },
        "401": {
          "description": "No inició sesión.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "./../schemas/schemas.json#/GeneralError"
              }
            }
          }
        },
        "403": {
          "description": "No dispone de los permisos suficientes para realizar esta acción.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "./../schemas/schemas.json#/GeneralError"
              }
            }
